pipeline{
    agent any
    
    stages{
        stage("Git Checkout"){
            steps{
            git 'https://github.com/sandeepmaheshwaram/myweb.git'
           
            }
        }
        stage("Mvn Build"){
            steps{
                sh "mvn clean package"
                sh "mv target/*.war target/myweb.war"
            }
        }
      
        }
    }
}
